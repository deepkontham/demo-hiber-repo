<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetAllController</servlet-name>
  	<display-name>GetAllController</display-name>
  	<description></description>
  	<servlet-class>com.getall.GetAllController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>All</servlet-name>
  	<display-name>All</display-name>
  	<description></description>
  	<servlet-class>com.getall.Allcontroller</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetAllController</servlet-name>
  	<url-pattern>/All</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Allcontroller</servlet-name>
  	<url-pattern>/A</url-pattern>
  </servlet-mapping>
</web-app>
